{  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
    ,
    "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
  }
  